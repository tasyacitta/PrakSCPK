% --- Executes on button press in pushProses.
function pushProses_Callback(hObject, eventdata, handles)
opts = detectImportOptions('DATA RUMAH.xlsx'); %memasukkan dataset yang akan digunakan
opts.SelectedVariableNames = [1 3:8]; %memilih range data yang akan digunakan
x = readmatrix('DATA RUMAH.xlsx', opts);%input data berdasarkan kriteria
k=[0,0,1,1,1,1,1];%nilai atribut, dimana 0= atribut biaya &1= atribut keuntungan
w=[0,0.3,0.2,0.23,0.10,0.07,0.10];% bobot untuk masing-masing kriteria
%tahapan 1. normalisasi matriks
[m n]=size (x); %matriks m x n dengan ukuran sebanyak variabel x (input)
R=zeros (m,n); %membuat matriks R, yang merupakan matriks kosong
Y=zeros (m,n); %membuat matriks Y, yang merupakan titik kosong
for j=1:n,
 if k(j)==1, %statement untuk kriteria dengan atribut keuntungan
  R(:,j)=x(:,j)./max(x(:,j));
 else
  R(:,j)=min(x(:,j))./x(:,j);
 end;
end;
%tahapan kedua, proses perangkingan
for i=1:m,
 V(i)= sum(w.*R(i,:)); %mencari nilai hasil
end;
[poin no]=sort(V,'descend'); %mengurutkan data berdasarkan nilai terbesar
%poin adalah nilai dari hasil perhitungan
%no adalah nomor urut data
%melakukan invers untuk 20 data teratas
    poin1=poin(:,1:20); %menyimpan hasil perhitungan
    poin2=no(:,1:20); %menyimpan nomor rumah yang sesuai kriteria
    poin3=[1:20;poin2;poin1];  
    set(handles.tableHasil,'data',(poin3).'); %menampilkan ranking rumah berdasarkan kesesuaian

% --- Executes on button press in pushData.
function pushData_Callback(hObject, eventdata, handles)
% hObject    handle to pushData (see GCBO)
% eventdata  reserved - to be defined in a future version of MATLAB
% handles    structure with handles and user data (see GUIDATA)
opts = detectImportOptions('DATA RUMAH.xlsx');
opts.SelectedVariableNames = [1 3:8];
data = readmatrix('DATA RUMAH.xlsx', opts);
set(handles.tableData,'data',data);
